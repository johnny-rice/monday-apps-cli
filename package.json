{
  "name": "monday-code-cli",
  "version": "0.1.1",
  "description": "A cli tool to manage monday-code projects in monday.com",
  "author": "monday.com Apps Team",
  "type": "module",
  "bin": {
    "mapps": "./bin/run.js"
  },
  "homepage": "https://github.com/mondaycom/monday-code-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "mondaycom/monday-code-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "start": "./bin/run.js",
    "dev": "./bin/dev.js",
    "build": "shx rm -rf dist && tsc -b && tsc-alias -f -p tsconfig.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "npm run lint -- --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/mondaycom/monday-code-cli/issues",
  "keywords": [
    "monday.com",
    "monday",
    "monday-code",
    "oclif",
    "cli"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/core": "^1.19.1",
    "@oclif/plugin-help": "^5",
    "archiver": "^5.3.1",
    "axios": "^1.2.1",
    "chalk": "4.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "figlet": "^1.5.2",
    "fuzzy": "^0.1.3",
    "glob": "^8.1.0",
    "http-status-codes": "^2.2.0",
    "inquirer": "^9.1.4",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "isemail": "^3.2.0",
    "nanospinner": "^1.1.0",
    "parse-gitignore": "^2.0.0",
    "pusher-js": "^8.0.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.12",
    "@types/archiver": "^5.3.1",
    "@types/figlet": "^1.5.5",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/parse-gitignore": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "jest": "^29.3.1",
    "oclif": "^3",
    "prettier": "^2.8.0",
    "shx": "^0.3.3",
    "ts-jest": "^29.0.3",
    "tsc-alias": "^1.8.6",
    "tslib": "^2.4.1",
    "tsx": "^3.12.7",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "mapps",
    "dirname": "mapps",
    "commands": "./dist/commands",
    "topics": {
      "code": {
        "description": "Perform operations related to apps in monday-code, monday.com's server hosting service"
      }
    },
    "hooks": {
      "init": "./dist/hooks/init.js"
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
